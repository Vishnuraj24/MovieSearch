{
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Topic": "movies.upserted",
    "GroupId": "movies-indexer",
    "SecurityProtocol": "PLAINTEXT"
  },
  "Elasticsearch": {
    "BaseUrl": "http://localhost:9200",
    "IndexName": "movies"
  },
  "Worker": {
    "PrintOnly": false // set true if you want to just print messages first
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
